{
    "name": "laravel-appkit/formulate",
    "description": "",
    "keywords": [
        "laravel",
        "appkit",
        "formulate"
    ],
    "homepage": "https://github.com/laravel-appkit/formulate",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Darren Coutts",
            "email": "d@coutts.io",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.37.0|^9.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.1",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^9.5",
        "symfony/css-selector": "^5.4",
        "symfony/dom-crawler": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "AppKit\\Formulate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppKit\\Formulate\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AppKit\\Formulate\\FormulateServiceProvider"
            ],
            "aliases": {
                "Formulate": "AppKit\\Formulate\\Facades\\Formulate"
            }
        }
    }
}
