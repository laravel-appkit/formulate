{
    "name": "laravel-appkit/formulate",
    "description": "",
    "keywords": [
        "laravel",
        "appkit",
        "formulate"
    ],
    "homepage": "https://github.com/laravel-appkit/formulate",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Darren Coutts",
            "email": "d@coutts.io",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "orchestra/testbench": "^6.24",
        "symfony/dom-crawler": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "laravel/legacy-factories": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "AppKit\\Formulate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppKit\\Formulate\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AppKit\\Formulate\\FormulateServiceProvider"
            ],
            "aliases": {
                "Formulate": "AppKit\\Formulate\\Facades\\Formulate"
            }
        }
    }
}
